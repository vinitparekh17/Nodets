{
  "name": "nodejs-typescript",
  "version": "1.0.0",
  "description": "Nodejs setup with typescript",
  "main": "./dist/app.js",
  "scripts": {
    "start": "tsc && node dist/app.js",
    "dev": "tsc && nodemon --exec node dist/app.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write ./src/**/*.{ts,js} && prettier --write ."
  },
  "keywords": [
    "nodejs",
    "typescript"
  ],
  "author": "Vinit Parekh",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "eslint": "^8.29.0",
    "prettier": "^2.8.1",
    "typescript": "^4.9.4"
  }
}
